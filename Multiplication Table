     using System;
      class Operator
      { 
          public static void Main()
        {
          int a , i ;
          Console.WriteLine("Enter the number");
          a = Convert.ToInt32(Console.ReadLine());
          for (i=1; i<=10;i++)
            {
               Console.WriteLine(a +" * "+ i +" = "+ (a*i));
            }       
        }
      }
